{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "linterOptions": {
      "exclude": ["node_modules/**"]
    },
    "jsRules": {
    },
    "rules": {
      "indent": [true, "spaces", 2],
      "quotemark": [true, "single", "jsx-double"],
      "semicolon": [true, "never"],
      "interface-name": [true, "always-prefix"],
      "object-literal-sort-keys": false
    },
    "rulesDirectory": []
}